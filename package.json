{
  "name": "webapp-framework7",
  "version": "2.0.0",
  "description": "Framework7 Vue Webpack starter app template",
  "scripts": {
    "build-dev": "node ./build/build-dev.js",
    "build-prod": "node ./build/build-prod.js",
    "dev": "webpack-dev-server --config ./build/webpack.config.dev.js",
    "prod": "webpack-dev-server --config ./build/webpack.config.prod.js",
    "start": "npm run dev",
    "copy-fonts": "cpy node_modules/framework7-icons/fonts/*.* src/style/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} src/style/fonts",
    "postinstall": "npm run copy-fonts",
    "mock": "node_modules/.bin/json-server --watch test/mock/db.json --port 3030",
    "mockdev": "npm run mock & npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framework7io/framework7-template-vue-webpack.git"
  },
  "keywords": [
    "ios",
    "webpack",
    "framework7",
    "vue",
    "vuejs",
    "material",
    "mobile",
    "app",
    "f7"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framework7io/framework7-template-vue-webpack/issues"
  },
  "homepage": "http://framework7.io/vue/",
  "dependencies": {
    "axios": "^0.18.0",
    "framework7": "^4.4.0",
    "framework7-icons": "^2.3.1",
    "framework7-vue": "^4.4.0",
    "hrkj-vux-components": "0.0.14",
    "material-design-icons": "^3.0.1",
    "vue": "^2.6.10",
    "vux": "^2.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.4.5",
    "autoprefixer": "^9.5.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^4.6.0",
    "cpy-cli": "^2.0.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "^0.15.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-notifier": "^5.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "portfinder": "^1.0.20",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
