{
  "name": "huiyishenyi",
  "version": "1.0.0",
  "description": "Framework7 Vue for RXT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "prod": "vue-cli-service build --mode production",
    "release": "vue-cli-service build --mode release",
    "start": "npm run dev",
    "copy-fonts": "cpy node_modules/framework7-icons/fonts/*.* src/style/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} src/style/fonts",
    "postinstall": "npm run copy-fonts",
    "mock": "node_modules/.bin/json-server --watch test/mock/db.json --port 3030",
    "mockdev": "npm run mock & npm run dev",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framework7io/framework7-template-vue-webpack.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "framework7": "^4.4.0",
    "framework7-icons": "^2.3.1",
    "framework7-vue": "^4.4.0",
    "hrkj-vux-components": "0.0.14",
    "material-design-icons": "^3.0.1",
    "vue": "^2.6.10",
    "vux": "^2.9.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vux/loader": "^2.0.0-rc4",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "cpy-cli": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "json-server": "^0.15.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "ora": "^3.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
